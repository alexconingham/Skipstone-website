{
  "name": "remember-to-die",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:prod": "next build && next export",
    "start": "next start",
    "lint": "next lint",
